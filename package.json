{
  "name": "tempo-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:mobile": "npm run start --workspace=@tempo/mobile",
    "dev:web": "npm run dev --workspace=@tempo/web",
    "build:web": "npm run build --workspace=@tempo/web",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.5.0"
  },
  "packageManager": "npm@10.8.2"
}
