# Convex - get these from your Convex project settings (https://convex.dev)
NEXT_PUBLIC_CONVEX_URL=https://your-project.convex.cloud
CONVEX_DEPLOY_KEY=your-deploy-key

# Clerk - get these from your Clerk dashboard (https://clerk.com)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your-key-here
CLERK_SECRET_KEY=sk_test_your-key-here

# AWS S3 (for PDF storage)
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=us-east-1
S3_BUCKET=tempo-cloud-pdfs

# Scan2Notes API (for OMR)
SCAN2NOTES_API_KEY=your-api-key

# Anthropic API - get from console.anthropic.com
ANTHROPIC_API_KEY=sk-ant-your-key-here
